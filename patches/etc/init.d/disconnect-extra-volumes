description="Disconnects additional OCS volumes"

depend() {
    after mount-ro fsck
    nbd-root-disconnect
}

start() {
    ebegin "Disconnecting extra volumes"
    /usr/local/sbin/oc-nbd-disconnect-extra
    eend $?
}
